#: model:mail.template,body_html:appointment.appointment_booked_mail_template
msgid ""
"<div style=\"font-size: 13px; color: #374151;\">\n"
"    <t t-set=\"recurrent\" t-value=\"object.recurrence_id and not "
"ctx.get('calendar_template_ignore_recurrence')\"/>\n"
"    <t t-set=\"mail_tz\" t-value=\"object._get_mail_tz() or "
"ctx.get('mail_tz')\"/>\n"
"    <t t-set=\"appointment_booker_name\" t-"
"value=\"object.appointment_booker_id.display_name\"/>\n"
"    <h1 style=\"font-size: 18px; font-weight: bold; color: #374151;\">\n"
"        <t t-if=\"not object.appointment_type_id.is_always_confirm and "
"object.appointment_status == 'request'\">Appointment requested</t>\n"
"        <t t-else=\"\">\n"
"            Booking confirmed\n"
"            <img src=\"/calendar/static/src/img/check-circle.png\" "
"style=\"border:0; width: 24px; height: 24px; margin-left: 10px; text-"
"decoration:none; vertical-align: bottom;\"/>\n"
"        </t>\n"
"    </h1>\n"
"    <p style=\"margin: 0;\">\n"
"        <span style=\"display: block; margin-top: 8px;\">\n"
"            <t t-out=\"appointment_booker_name or ''\">John Booker</t>\n"
"            <t t-if=\"not object.appointment_type_id.is_always_confirm and "
"object.appointment_status == 'request'\"> has requested </t>\n"
"            <t t-else=\"\"> has booked </t>\n"
"            an appointment: <t t-out=\"object.appointment_type_id.name or "
"''\">Technical Demo</t>\n"
"            <t t-if=\"object.appointment_type_id.category not in ['custom', "
"'anytime'] and object.appointment_type_id.schedule_based_on == 'users' and "
"object.partner_id\">\n"
"                with <t t-out=\"object.partner_id.name or ''\">Brandon "
"Freeman</t>\n"
"            </t>.\n"
"        </span>\n"
"    </p>\n"
"    <div style=\"text-align: center; padding: 16px 0px 16px 0px; margin-top: "
"16px;\">\n"
"        <a t-attf-href=\"/calendar/meeting/join?"
"token={{ object.access_token }}\" t-attf-style=\"display: inline-block; "
"padding: 7px 14px; color: {{user.company_id.email_primary_color or "
"'#FFFFFF'}}; text-decoration: none; background-color: "
"{{user.company_id.email_secondary_color or '#875A7B'}}; border-radius: "
"3px\">\n"
"            Join</a>\n"
"        <a t-attf-href=\"/odoo/calendar.event/{{ object.id }}\" t-attf-"
"style=\"display: inline-block; margin-left: 12px; padding: 7px 14px; color: "
"#111827; text-decoration: none; background-color: #E7E9ED; border-radius: "
"3px;\">\n"
"            View</a>\n"
"    </div>\n"
"    <div style=\"margin: 16px 0 0;\">\n"
"        <h2 t-attf-style=\"margin-bottom: 12px; font-size: 14px; font-"
"weight: bold; color: {{user.company_id.email_secondary_color or '#875A7B'}};"
"\">Details</h2>\n"
"        <table cellpadding=\"0\" cellspacing=\"0\" border=\"0\" style=\"font-"
"size: 13px; color: #374151;\">\n"
"            <tr>\n"
"                <td style=\"width: 100px; color: #676F7B;\">Date &amp; Time</"
"td>\n"
"                <td style=\"font-weight: bold;\">\n"
"                    <t t-out=\"format_datetime(dt=object.start, tz=mail_tz "
"if not object.allday else None, dt_format='EEEE', lang_code=object.env.lang) "
"or ''\">Wednesday</t>\n"
"                    <t t-out=\"format_datetime(dt=object.start, tz=mail_tz "
"if not object.allday else None, dt_format='d', lang_code=object.env.lang) or "
"''\">1</t>\n"
"                    <t t-out=\"format_datetime(dt=object.start, tz=mail_tz "
"if not object.allday else None, dt_format='MMMM y', "
"lang_code=object.env.lang) or ''\">January 2020</t>\n"
"                    <t t-if=\"not object.allday\">\n"
"                        <t t-out=\"format_time(time=object.start, "
"tz=mail_tz, time_format='short', lang_code=object.env.lang) or ''\">8:00</"
"t>\n"
"                        <t t-if=\"not "
"object.appointment_type_id.hide_timezone and mail_tz\">\n"
"                            <span style=\"font-size: 11px; font-weight: "
"normal;\">\n"
"                                (<t t-out=\"mail_tz\"/>)\n"
"                            </span>\n"
"                        </t>\n"
"                    </t>\n"
"                </td>\n"
"            </tr>\n"
"            <tr t-if=\"recurrent\">\n"
"                <td style=\"width: 100px; padding-top: 6px; color: #676F7B;"
"\">When</td>\n"
"                <td style=\"padding-top: 6px;\"><t t-"
"out=\"object.recurrence_id.name or ''\">Every 1 Weeks, for 3 events</t></"
"td>\n"
"            </tr>\n"
"            <tr t-if=\"not object.appointment_type_id.hide_duration and not "
"object.allday and object.duration\">\n"
"                <td style=\"width: 100px; padding-top: 6px; color: #676F7B;"
"\">Duration</td>\n"
"                <td style=\"padding-top: 6px;\"><t t-out=\"('%dH%02d' % "
"(object.duration,round(object.duration*60)%60)) or ''\">0H30</t></td>\n"
"            </tr>\n"
"            <tr t-if=\"object.location\">\n"
"                <td style=\"width: 100px; padding-top: 6px; color: #676F7B; "
"vertical-align: top;\">Location</td>\n"
"                <td style=\"padding-top: 6px;\">\n"
"                    <t t-out=\"object.location or ''\">Bruxelles</t>\n"
"                    <a target=\"_blank\" t-if=\"object.location != "
"object.appointment_type_id.location_id.name\" t-attf-href=\"http://"
"maps.google.com/maps?oi=map&amp;q={{ object.location }}\" style=\"margin-"
"left: 12px; font-size: 12px; color: #008f8c; text-decoration: underline;"
"\">View Map</a>\n"
"                </td>\n"
"            </tr>\n"
"            <tr t-if=\"object.appointment_type_id.manage_capacity\">\n"
"                <td style=\"width: 100px; padding-top: 6px; color: #676F7B;"
"\">For</td>\n"
"                <td style=\"padding-top: 6px;\"><t t-"
"out=\"object.total_capacity_reserved\"/> people</td>\n"
"            </tr>\n"
"            <tr t-if=\"not object.appointment_type_id.is_auto_assign and "
"object.appointment_resource_ids\">\n"
