#. module: appointment
#: model:mail.template,body_html:appointment.attendee_invitation_mail_template
msgid ""
"<div style=\"font-size: 13px; color: #374151;\">\n"
"    <t t-set=\"customer\" t-value=\" object.event_id.find_partner_customer()"
"\"/>\n"
"    <t t-set=\"target_responsible\" t-value=\"object.partner_id == "
"object.event_id.partner_id\"/>\n"
"    <t t-set=\"target_customer\" t-value=\"object.partner_id == customer\"/"
">\n"
"    <t t-set=\"recurrent\" t-value=\"object.recurrence_id and not "
"ctx.get('calendar_template_ignore_recurrence')\"/>\n"
"\n"
"    <h1 style=\"font-size: 18px; font-weight: bold; color: #374151; margin-"
"bottom: 28px;\">\n"
"        Invitation\n"
"    </h1>\n"
"    <p style=\"margin: 0;\">\n"
"        <span style=\"display: block; margin-bottom: 8px;\">Hello <t t-"
"out=\"object.common_name or ''\">Wood Corner</t>,</span>\n"
"\n"
"        <t t-if=\"target_customer\">\n"
"            <t t-if=\"not "
"object.event_id.appointment_type_id.is_always_confirm\">\n"
"                <t t-if=\"object.event_id.appointment_status == 'booked'\">\n"
"                    We're happy to let you know your booking <strong t-"
"out=\"object.event_id.appointment_type_id.name or ''\">Schedule a Demo</"
"strong><t t-if=\"object.event_id.appointment_type_id.category not in "
"['custom', 'anytime'] and "
"object.event_id.appointment_type_id.schedule_based_on == 'users'\"> with <t "
"t-out=\"object.event_id.user_id.name or ''\">Ready Mat</t></t> has been "
"confirmed.<br/>\n"
"                </t>\n"
"                <t t-elif=\"object.event_id.appointment_status == "
"'request'\">\n"
"                    We've got your booking <strong t-"
"out=\"object.event_id.appointment_type_id.name or ''\">Schedule a Demo</"
"strong><t t-if=\"object.event_id.appointment_type_id.category not in "
"['custom', 'anytime'] and "
"object.event_id.appointment_type_id.schedule_based_on == 'users'\"> with <t "
"t-out=\"object.event_id.user_id.name or ''\">Ready Mat</t></t>.<br/>\n"
"                    We'll notify you once it's confirmed.\n"
"                </t>\n"
"            </t>\n"
"            <t t-else=\"\">\n"
"                Your appointment <strong t-"
"out=\"object.event_id.appointment_type_id.name or ''\">Schedule a Demo</"
"strong> <t t-if=\"object.event_id.appointment_type_id.category not in "
"['custom', 'anytime'] and "
"object.event_id.appointment_type_id.schedule_based_on == 'users'\"> with <t "
"t-out=\"object.event_id.user_id.name or ''\">Ready Mat</t></t> has been "
"booked.\n"
"            </t>\n"
"        </t>\n"
"        <t t-elif=\"target_responsible\">\n"
"            <t t-if=\"customer\">\n"
"                <t t-out=\"customer.name or ''\"/> scheduled the following "
"appointment <strong t-out=\"object.event_id.appointment_type_id.name or "
"''\">Schedule a Demo</strong> with you.\n"
"                <t t-if=\"object.event_id.appointment_type_id and "
"object.event_id.appointment_status == 'request'\">\n"
"                    It is awaiting confirmation.\n"
"                </t>\n"
"            </t>\n"
"            <t t-else=\"\">\n"
"                Your appointment <strong t-"
"out=\"object.event_id.appointment_type_id.name or ''\">Schedule a Demo</"
"strong> has been booked.\n"
"            </t>\n"
"        </t>\n"
"        <t t-else=\"\">\n"
"            You have been invited to the following appointment <strong t-"
"out=\"object.event_id.appointment_type_id.name or ''\">Schedule a Demo</"
"strong>.\n"
"        </t>\n"
"    </p>\n"
"    <div style=\"text-align: center; padding: 16px 0px 16px 0px;\">\n"
"        <t t-if=\"object.state != 'accepted'\">\n"
"            <a t-attf-href=\"/calendar/meeting/accept?"
"token={{object.access_token}}&amp;id={{object.event_id.id}}\" t-attf-"
"style=\"display: inline-block; padding: 7px 14px; color: "
"{{user.company_id.email_primary_color or '#FFFFFF'}}; text-decoration: none; "
"background-color: {{user.company_id.email_secondary_color or '#875A7B'}}; "
"border-radius: 3px\">\n"
"                Accept</a>\n"
"            <a t-attf-href=\"/calendar/meeting/decline?"
"token={{object.access_token}}&amp;id={{object.event_id.id}}\" t-attf-"
"style=\"display: inline-block; margin-left: 6px; padding: 7px 14px; color: "
"{{user.company_id.email_primary_color or '#FFFFFF'}}; text-decoration: none; "
"background-color: {{user.company_id.email_secondary_color or '#875A7B'}}; "
"border-radius: 3px\">\n"
"                Decline</a>\n"
"        </t>\n"
"        <a t-if=\"not target_responsible and not target_customer\" t-attf-"
"href=\"/calendar/meeting/view?token={{object.access_token}}"
"&amp;id={{object.event_id.id}}\" style=\"display: inline-block; margin-left: "
"12px; padding: 5px 10px; color: #111827; text-decoration: none; background-"
"color: #E7E9ED; border-radius: 3px; border-radius: 3px\">View</a>\n"
"        <a t-elif=\"not target_customer\" t-attf-href=\"/mail/view?"
"model=calendar.event&amp;res_id={{object.event_id.id}}\" t-attf-"
"style=\"display: inline-block; margin-left: 12px; padding: 5px 10px; color: "
"#111827; text-decoration: none; background-color: #E7E9ED; border-radius: "
"3px; border-radius: 3px\">View</a>\n"
"    </div>\n"
"    <div style=\"margin: 16px 0 0;\">\n"
"        <h2 t-attf-style=\"margin-bottom: 12px; font-size: 14px; font-"
"weight: bold; color: {{user.company_id.email_secondary_color or '#875A7B'}};"
"\">Details</h2>\n"
"        <table cellpadding=\"0\" cellspacing=\"0\" border=\"0\" style=\"font-"
"size: 13px; color: #374151;\">\n"
"            <tr>\n"
"                <td style=\"width: 100px; color: #676F7B;\">Date &amp; Time</"
"td>\n"
"                <td style=\"font-weight: bold;\">\n"
"                    <t t-out=\"format_datetime(dt=object.event_id.start, "
"tz=object.mail_tz if not object.event_id.allday else None, dt_format='EEEE', "
"lang_code=object.env.lang) or ''\">Tuesday</t>\n"
"                    <t t-out=\"format_datetime(dt=object.event_id.start, "
"tz=object.mail_tz if not object.event_id.allday else None, dt_format='d', "
"lang_code=object.env.lang) or ''\">4</t>\n"
"                    <t t-out=\"format_datetime(dt=object.event_id.start, "
"tz=object.mail_tz if not object.event_id.allday else None, dt_format='MMMM "
"y', lang_code=object.env.lang) or ''\">May 2021</t>\n"
"                    <t t-if=\"not object.event_id.allday\">\n"
"                        <t t-out=\"format_time(time=object.event_id.start, "
"tz=object.mail_tz, time_format='short', lang_code=object.env.lang) or "
"''\">11:00 AM</t>\n"
"                        <t t-if=\"not "
"object.event_id.appointment_type_id.hide_timezone and object.mail_tz\">\n"
"                            <span style=\"font-size: 11px; font-weight: "
"normal;\">\n"
"                                (<t t-out=\"object.mail_tz or ''\">Europe/"
"Brussels</t>)\n"
"                            </span>\n"
"                        </t>\n"
"                    </t>\n"
"                </td>\n"
"            </tr>\n"
"            <tr t-if=\"recurrent\">\n"
"                <td style=\"width: 100px; padding-top: 6px; color: #676F7B;"
"\">When</td>\n"
"                <td style=\"padding-top: 6px;\"><t t-"
"out=\"object.recurrence_id.name or ''\">Every 1 Weeks, for 3 events</t></"
"td>\n"
"            </tr>\n"
"            <tr t-if=\"not object.event_id.appointment_type_id.hide_duration "
"and not object.event_id.allday and object.event_id.duration\">\n"
"                <td style=\"width: 100px; padding-top: 6px; color: #676F7B;"
"\">Duration</td>\n"
"                <td style=\"padding-top: 6px;\"><t t-out=\"('%dH%02d' % "
"(object.event_id.duration,round(object.event_id.duration*60)%60)) or "
"''\">0H30</t></td>\n"
"            </tr>\n"
"            <tr t-if=\"object.event_id.location\">\n"
"                <td style=\"width: 100px; padding-top: 6px; color: #676F7B; "
"vertical-align: top;\">Location</td>\n"
"                <td style=\"padding-top: 6px;\">\n"
"                    <t t-out=\"object.event_id.location or ''\">Bruxelles</"
"t>\n"
"                    <a target=\"_blank\" t-if=\"object.event_id.location != "
"object.event_id.appointment_type_id.location_id.name\" t-attf-href=\"http://"
"maps.google.com/maps?oi=map&amp;q={{object.event_id.location}}\" "
"style=\"margin-left: 12px; font-size: 12px; color: #008f8c; text-decoration: "
"underline;\">View Map</a>\n"
"                </td>\n"
"            </tr>\n"
"            <tr t-"
"if=\"object.event_id.appointment_type_id.manage_capacity\">\n"
"                <td style=\"width: 100px; padding-top: 6px; color: #676F7B;"
"\">For</td>\n"
"                <td style=\"padding-top: 6px;\"><t t-"
"out=\"object.event_id.total_capacity_reserved\"/> people</td>\n"
"            </tr>\n"
"            <tr t-if=\"not "
"object.event_id.appointment_type_id.is_auto_assign and "
"object.event_id.appointment_resource_ids\">\n"
"                <td style=\"width: 100px; padding-top: 6px; color: #676F7B; "
"vertical-align: top;\">Resources</td>\n"
"                <td style=\"padding-top: 6px;\">\n"
"                    <ul style=\"padding: 0 15px;\">\n"
"                        <li t-"
"foreach=\"object.event_id.appointment_resource_ids\" t-as=\"resource\">\n"
"                            <span style=\"margin-left:5px\" t-"
"out=\"resource.name or ''\">Table 1</span>\n"
"                        </li>\n"
"                    </ul>\n"
"                </td>\n"
"            </tr>\n"
"            <tr t-if=\"object.event_id.videocall_redirection\">\n"
"                <td style=\"width: 100px; padding-top: 6px; color: #676F7B;"
"\">\n"
"                    <t t-if=\"object.event_id.videocall_source == "
"'discuss'\">Join with</t>\n"
"                    <t t-else=\"\">Join</t>\n"
"                </td>\n"
"                <td style=\"padding-top: 6px;\">\n"
"                    <a t-attf-"
"href=\"{{ object.event_id.videocall_redirection }}\" target=\"_blank\" "
"style=\"color: #008f8c;\">\n"
"                        <t t-if=\"object.event_id.videocall_source == "
"'discuss'\">Odoo Discuss</t>\n"
"                        <t t-else=\"\">Video meeting</t>\n"
"                    </a>\n"
"                </td>\n"
"            </tr>\n"
"        </table>\n"
"    </div>\n"
"    <div style=\"margin: 32px 0 0;\">\n"
"        <h2 t-attf-style=\"margin-bottom: 12px; font-size: 14px; font-"
"weight: bold; color: {{user.company_id.email_secondary_color or '#875A7B'}};"
"\">Attendees</h2>\n"
"        <div t-foreach=\"object.event_id.attendee_ids\" t-as=\"attendee\" "
"style=\"margin-bottom: 5px;\">\n"
"            <img t-if=\"attendee.state\" t-attf-src=\"/calendar/static/src/"
"img/state_{{ attendee.state }}.png\" style=\"border:0; width: 15px; height: "
"15px; vertical-align: text-bottom;\"/>\n"
"            <t t-if=\"attendee != object\">\n"
"                <span style=\"margin-left: 5px\" t-"
"out=\"attendee.common_name or ''\">Mitchell Admin</span>\n"
"            </t>\n"
"            <t t-else=\"\">\n"
"                <span style=\"margin-left:5px\">You</span>\n"
"            </t>\n"
"        </div>\n"
"    </div>\n"
"    <div t-if=\"not is_html_empty(object.event_id.description)\" "
"style=\"margin: 32px 0 0;\">\n"
"        <h2 t-attf-style=\"margin-bottom: 12px; font-size: 14px; font-"
"weight: bold; color: {{user.company_id.email_secondary_color or '#875A7B'}};"
"\">Description of the event</h2>\n"
"        <div t-out=\"object.event_id.description\"/>\n"
"    </div>\n"
"    <div t-if=\"target_customer\" style=\"margin: 32px 0 0;\">\n"
"        <h2 t-attf-style=\"margin-bottom: 12px; font-size: 14px; font-"
"weight: bold; color: {{user.company_id.email_secondary_color or '#875A7B'}};"
"\">Make change to this event</h2>\n"
"        <a t-attf-href=\"/calendar/meeting/view?token={{object.access_token}}"
"&amp;id={{object.event_id.id}}\" style=\"color: color: #008f8c; text-"
"decoration: underline;\">Reschedule</a>\n"
"    </div>\n"
"    <t t-set=\"upcoming_appointments\" t-"
"value=\"(object.event_id.appointment_booker_id.upcoming_appointment_ids - "
"object.event_id).sorted('start')\"/>\n"
"    <div t-if=\"upcoming_appointments\" style=\"margin: 32px 0 0;\">\n"
"        <h2 t-attf-style=\"margin-bottom: 12px; font-size: 14px; font-"
"weight: bold; color: {{user.company_id.email_secondary_color or '#875A7B'}};"
"\">Your Other Upcoming Appointment(s)</h2>\n"
"        <ul style=\"margin: 6px 0 0; padding: 0 18px;\">\n"
"            <li style=\"margin-top: 6px;\" t-"
"foreach=\"upcoming_appointments\" t-as=\"upcoming_appointment\">\n"
"                <span t-out=\"upcoming_appointment.appointment_type_id.name "
"or ''\">Technical Demo</span>\n"
"                <span style=\"margin: 0 10px;\">-</span>\n"
"                <span t-out=\"upcoming_appointment.start or ''\" t-"
"options=\"{&quot;widget&quot;: &quot;datetime&quot;, &quot;format&quot;: "
"&quot;medium&quot;, &quot;tz_name&quot;: object.mail_tz}\" style=\"font-"
"weight: bold;\">June 20th, 15:00</span>\n"
"                <a t-attf-href=\"/calendar/view/"
"#{upcoming_appointment.access_token}?partner_id=#{appointment_booker_id}\" "
"target=\"_blank\" style=\"margin-left: 10px; font-size: 12px; color: "
"#008f8c; text-decoration: underline;\">View Details</a>\n"
"            </li>\n"
"        </ul>\n"
"    </div>\n"
"    <p style=\"margin: 32px 0 0;\">\n"
"        Thank you!\n"
"        <t t-if=\"object.event_id.user_id.signature\">\n"
"            <span style=\"display: block; margin-top: 8px;\"><t t-"
"out=\"object.event_id.user_id.signature or ''\">Mitchell Admin</t></span>\n"
"        </t>\n"
"    </p>\n"
"</div>\n"
"            "
msgstr ""
"<div style=\"font-size: 13px; color: #374151;\">\n"
"    <t t-set=\"customer\" t-value="
"\" object.event_id.find_partner_customer()\"/>\n"
"    <t t-set=\"target_responsible\" t-value="
"\"object.partner_id == object.event_id.partner_id\"/>\n"
"    <t t-set=\"target_customer\" t-value=\"object.partner_id == customer\"/>"
"\n"
"    <t t-set=\"recurrent\" t-value=\"object.recurrence_id and not ctx.get"
"('calendar_template_ignore_recurrence')\"/>\n"
"\n"
"    <h1 style=\"font-size: 18px; font-weight: bold; color: #374151; margin-"
"bottom: 28px;\">\n"
"        Undangan\n"
"    </h1>\n"
"    <p style=\"margin: 0;\">\n"
"        <span style=\"display: block; margin-bottom: 8px;\">Halo <t t-out="
"\"object.common_name or ''\">Wood Corner</t>,</span>\n"
"\n"
"        <t t-if=\"target_customer\">\n"
"            <t t-if="
"\"not object.event_id.appointment_type_id.is_always_confirm\">\n"
"                <t t-if=\"object.event_id.appointment_status == 'booked'\">\n"
"                    Dengan bahagia kami memberi kabar bahwa booking Anda "
"<strong t-out=\"object.event_id.appointment_type_id.name or ''\">Jadwalkan "
"emo</strong><t t-if=\"object.event_id.appointment_type_id.category not in "
"['custom', 'anytime'] and "
"object.event_id.appointment_type_id.schedule_based_on == 'users'"
"\"> dengan <t t-out=\"object.event_id.user_id.name or ''\">Ready Mat</t></t> "
"telah dikonfirmasi.<br/>\n"
"                </t>\n"
"                <t t-elif=\"object.event_id.appointment_status == 'request'\""
">\n"
"                    Kami mendapatkan booking Anda <strong t-out="
"\"object.event_id.appointment_type_id.name or ''\">Jadwalkan Demo</strong><t "
"t-if=\"object.event_id.appointment_type_id.category not in "
"['custom', 'anytime'] and "
"object.event_id.appointment_type_id.schedule_based_on == 'users'"
"\"> dengan <t t-out=\"object.event_id.user_id.name or ''\">Ready "
"Mat</t></t>.<br/>\n"
"                    Kami akan menotifikasi ANda setelah dikonfirmasi.\n"
"                </t>\n"
"            </t>\n"
"            <t t-else=\"\">\n"
"                Appointment Anda <strong t-out="
"\"object.event_id.appointment_type_id.name or ''\">Jadwalkan Demo</strong> "
"<t t-if=\"object.event_id.appointment_type_id.category not in "
"['custom', 'anytime'] and "
"object.event_id.appointment_type_id.schedule_based_on == 'users'"
"\"> dengan <t t-out=\"object.event_id.user_id.name or ''\">Ready Mat</t></t> "
"telah dibook.\n"
"            </t>\n"
"        </t>\n"
"        <t t-elif=\"target_responsible\">\n"
"            <t t-if=\"customer\">\n"
"                <t t-out=\"customer.name or ''\"/> menjadwalkan appointment "
"berikut <strong t-out=\"object.event_id.appointment_type_id.name or ''\""
">Jadwalkan Demo</strong> dengan Anda.\n"
"                <t t-if=\"object.event_id.appointment_type_id and "
"object.event_id.appointment_status == 'request'\">\n"
"                    Telah menunggu konfirmasi.\n"
"                </t>\n"
"            </t>\n"
"            <t t-else=\"\">\n"
"                Appointment Anda <strong t-out="
"\"object.event_id.appointment_type_id.name or ''\">Jadwalkan Demo</strong> "
"telah dibook.\n"
"            </t>\n"
"        </t>\n"
"        <t t-else=\"\">\n"
"            Anda telah diundang ke appointment berikut <strong t-out="
"\"object.event_id.appointment_type_id.name or ''\">Jadwalkan Demo</strong>.\n"
"        </t>\n"
"    </p>\n"
"    <div style=\"text-align: center; padding: 16px 0px 16px 0px;\">\n"
"        <t t-if=\"object.state != 'accepted'\">\n"
"            <a t-attf-href=\"/calendar/meeting/"
"accept?token={{object.access_token}}&amp;id={{object.event_id.id}}"
"\" t-attf-style=\"display: inline-block; padding: 7px 14px; color: {{"
"user.company_id.email_primary_color or '#FFFFFF'}}; text-decoration: none; "
"background-color: {{user.company_id.email_secondary_color or '#875A7B'}}; "
"border-radius: 3px\">\n"
"                Terima</a>\n"
"            <a t-attf-href=\"/calendar/meeting/"
"decline?token={{object.access_token}}&amp;id={{object.event_id.id}}"
"\" t-attf-style=\"display: inline-block; margin-left: 6px; padding: 7px 14px;"
" color: {{user.company_id.email_primary_color or '#FFFFFF'}}; text-"
"decoration: none; background-color: {{user.company_id.email_secondary_color "
"or '#875A7B'}}; border-radius: 3px\">\n"
"                Tolak</a>\n"
"        </t>\n"
"        <a t-if=\"not target_responsible and not target_customer\" t-attf-"
"href=\"/calendar/meeting/"
"view?token={{object.access_token}}&amp;id={{object.event_id.id}}\" style=\""
"display: inline-block; margin-left: 12px; padding: 5px 10px; color: #111827; "
"text-decoration: none; background-color: #E7E9ED; border-radius: 3px; border-"
"radius: 3px\">Lihat</a>\n"
"        <a t-elif=\"not target_customer\" t-attf-href="
"\"/mail/view?model=calendar.event&amp;res_id={{object.event_id.id}}\" t-attf-"
"style=\"display: inline-block; margin-left: 12px; padding: 5px 10px; color: "
"#111827; text-decoration: none; background-color: #E7E9ED; border-radius: "
"3px; border-radius: 3px\">Lihat</a>\n"
"    </div>\n"
"    <div style=\"margin: 16px 0 0;\">\n"
"        <h2 t-attf-style=\"margin-bottom: 12px; font-size: 14px; font-weight:"
" bold; color: {{user.company_id.email_secondary_color or '#875A7B'}};\""
">Detail</h2>\n"
"        <table cellpadding=\"0\" cellspacing=\"0\" border=\"0\" style=\"font-"
"size: 13px; color: #374151;\">\n"
"            <tr>\n"
"                <td style=\"width: 100px; color: #676F7B;\">Tanggal &amp; "
"Waktu</td>\n"
"                <td style=\"font-weight: bold;\">\n"
"                    <t t-out=\"format_datetime(dt=object.event_id.start, "
"tz=object.mail_tz if not object.event_id.allday else None, dt_format='EEEE', "
"lang_code=object.env.lang) or ''\">Selasa</t>\n"
"                    <t t-out=\"format_datetime(dt=object.event_id.start, "
"tz=object.mail_tz if not object.event_id.allday else None, dt_format='d', "
"lang_code=object.env.lang) or ''\">4</t>\n"
"                    <t t-out=\"format_datetime(dt=object.event_id.start, "
"tz=object.mail_tz if not object.event_id.allday else None, dt_format='MMMM "
"y', lang_code=object.env.lang) or ''\">Mei 2021</t>\n"
"                    <t t-if=\"not object.event_id.allday\">\n"
"                        <t t-out=\"format_time(time=object.event_id.start, "
"tz=object.mail_tz, time_format='short', lang_code=object.env.lang) or ''\""
">11:00 AM</t>\n"
"                        <t t-if="
"\"not object.event_id.appointment_type_id.hide_timezone and object.mail_tz\">"
"\n"
"                            <span style=\"font-size: 11px; font-weight: "
"normal;\">\n"
"                                "
"(<t t-out=\"object.mail_tz or ''\">Eropa/Brussels</t>)\n"
"                            </span>\n"
"                        </t>\n"
"                    </t>\n"
"                </td>\n"
"            </tr>\n"
"            <tr t-if=\"recurrent\">\n"
"                <td style=\"width: 100px; padding-top: 6px; color: #676F7B;\""
">Kapan</td>\n"
"                <td style=\"padding-top: 6px;\"><t t-out=\""
"object.recurrence_id.name or ''\">Setiap 1 Minggu, untuk 3 acara</t></td>\n"
"            </tr>\n"
"            <tr t-if=\"not object.event_id.appointment_type_id.hide_duration "
"and not object.event_id.allday and object.event_id.duration\">\n"
"                <td style=\"width: 100px; padding-top: 6px; color: #676F7B;\""
">Durasi</td>\n"
"                <td style=\"padding-top: 6px;\"><t t-out=\""
"('%dH%02d' % (object.event_id.duration,round(object.event_id.duration*60)"
"%60)) or ''\">0J30M</t></td>\n"
"            </tr>\n"
"            <tr t-if=\"object.event_id.location\">\n"
"                <td style="
"\"width: 100px; padding-top: 6px; color: #676F7B; vertical-align: top;\""
">Lokasi</td>\n"
"                <td style=\"padding-top: 6px;\">\n"
"                    <t t-out=\"object.event_id.location or ''\">Bruxelles</t>"
"\n"
"                    <a target=\"_blank\" t-if=\"object.event_id.location != "
"object.event_id.appointment_type_id.location_id.name\" t-attf-href="
"\"http://maps.google.com/maps?oi=map&amp;q={{object.event_id.location}}\" "
"style=\"margin-left: 12px; font-size: 12px; color: #008f8c; text-decoration: "
"underline;\">Lihat Peta</a>\n"
"                </td>\n"
"            </tr>\n"
"            <tr t-if=\"object.event_id.appointment_type_id.manage_capacity\">"
"\n"
"                <td style=\"width: 100px; padding-top: 6px; color: #676F7B;\""
">Untuk</td>\n"
"                <td style=\"padding-top: 6px;\"><t t-out=\"object.event_id."
"total_capacity_reserved\"/> orang</td>\n"
"            </tr>\n"
"            <tr t-if=\"not "
"object.event_id.appointment_type_id.is_auto_assign and object.event_id."
"appointment_resource_ids\">\n"
"                <td style="
"\"width: 100px; padding-top: 6px; color: #676F7B; vertical-align: top;\""
">Sumber Daya</td>\n"
"                <td style=\"padding-top: 6px;\">\n"
"                    <ul style=\"padding: 0 15px;\">\n"
"                        <li t-foreach="
"\"object.event_id.appointment_resource_ids\" t-as=\"resource\">\n"
"                            <span style=\"margin-left:5px\" t-out="
"\"resource.name or ''\">Meja 1</span>\n"
"                        </li>\n"
"                    </ul>\n"
"                </td>\n"
"            </tr>\n"
"            <tr t-if=\"object.event_id.videocall_redirection\">\n"
"                <td style=\"width: 100px; padding-top: 6px; color: #676F7B;\""
">\n"
"                    <t t-if=\"object.event_id.videocall_source == 'discuss'\""
">Bergabung dengan</t>\n"
"                    <t t-else=\"\">Bergabung</t>\n"
"                </td>\n"
"                <td style=\"padding-top: 6px;\">\n"
"                    <a t-attf-href="
"\"{{ object.event_id.videocall_redirection }}\" target=\"_blank\" style="
"\"color: #008f8c;\">\n"
"                        <t t-if="
"\"object.event_id.videocall_source == 'discuss'\">Odoo Discuss</t>\n"
"                        <t t-else=\"\">Video meeting</t>\n"
"                    </a>\n"
"                </td>\n"
"            </tr>\n"
"        </table>\n"
"    </div>\n"
"    <div style=\"margin: 32px 0 0;\">\n"
"        <h2 t-attf-style=\"margin-bottom: 12px; font-size: 14px; font-weight:"
" bold; color: {{user.company_id.email_secondary_color or '#875A7B'}};\""
">Peserta</h2>\n"
"        <div t-foreach=\"object.event_id.attendee_ids\" t-as=\"attendee\" "
"style=\"margin-bottom: 5px;\">\n"
"            <img t-if=\"attendee.state\" t-attf-src="
"\"/calendar/static/src/img/state_{{ attendee.state }}.png\" style="
"\"border:0; width: 15px; height: 15px; vertical-align: text-bottom;\"/>\n"
"            <t t-if=\"attendee != object\">\n"
"                <span style=\"margin-left: 5px\" t-out="
"\"attendee.common_name or ''\">Mitchell Admin</span>\n"
"            </t>\n"
"            <t t-else=\"\">\n"
"                <span style=\"margin-left:5px\">Anda</span>\n"
"            </t>\n"
"        </div>\n"
"    </div>\n"
"    <div t-if=\"not is_html_empty(object.event_id.description)\" style="
"\"margin: 32px 0 0;\">\n"
"        <h2 t-attf-style=\"margin-bottom: 12px; font-size: 14px; font-weight:"
" bold; color: {{user.company_id.email_secondary_color or '#875A7B'}};\""
">Keterangan acara</h2>\n"
"        <div t-out=\"object.event_id.description\"/>\n"
"    </div>\n"
"    <div t-if=\"target_customer\" style=\"margin: 32px 0 0;\">\n"
"        <h2 t-attf-style=\"margin-bottom: 12px; font-size: 14px; font-weight:"
" bold; color: {{user.company_id.email_secondary_color or '#875A7B'}};\">Buat "
"perubahan ke acara ini</h2>\n"
"        <a t-attf-href=\"/calendar/meeting/"
"view?token={{object.access_token}}&amp;id={{object.event_id.id}}\" style=\""
"color: color: #008f8c; text-decoration: underline;\">Jadwalkan ulang</a>\n"
"    </div>\n"
"    <t t-set=\"upcoming_appointments\" t-value=\"("
"object.event_id.appointment_booker_id.upcoming_appointment_ids - "
"object.event_id).sorted('start')\"/>\n"
"    <div t-if=\"upcoming_appointments\" style=\"margin: 32px 0 0;\">\n"
"        <h2 t-attf-style=\"margin-bottom: 12px; font-size: 14px; font-weight:"
" bold; color: {{user.company_id.email_secondary_color or '#875A7B'}};\""
">Appointment Mendatang Anda Lainnya</h2>\n"
"        <ul style=\"margin: 6px 0 0; padding: 0 18px;\">\n"
"            <li style=\"margin-top: 6px;\" t-foreach=\""
"upcoming_appointments\" t-as=\"upcoming_appointment\">\n"
"                <span t-out="
"\"upcoming_appointment.appointment_type_id.name or ''\">Demo Teknis</span>\n"
"                <span style=\"margin: 0 10px;\">-</span>\n"
"                <span t-out=\"upcoming_appointment.start or ''\" t-options=\""
"{&quot;widget&quot;: &quot;datetime&quot;, &quot;format&quot;: "
"&quot;medium&quot;, &quot;tz_name&quot;: object.mail_tz}\" style=\"font-"
"weight: bold;\">Juni 20, 15:00</span>\n"
"                <a t-attf-href=\"/calendar/view/"
"#{upcoming_appointment.access_token}?partner_id=#{appointment_booker_id}"
"\" target=\"_blank\" style=\"margin-left: 10px; font-size: 12px; color: "
"#008f8c; text-decoration: underline;\">Lihat Detail</a>\n"
"            </li>\n"
"        </ul>\n"
"    </div>\n"
"    <p style=\"margin: 32px 0 0;\">\n"
"        Terima kasih!\n"
"        <t t-if=\"object.event_id.user_id.signature\">\n"
"            <span style=\"display: block; margin-top: 8px;\"><t t-out="
"\"object.event_id.user_id.signature or ''\">Mitchell Admin</t></span>\n"
"        </t>\n"
"    </p>\n"
"</div>\n"
"            "

#. module: appointment
#: model:mail.template,body_html:appointment.appointment_canceled_mail_template
msgid ""
"<div style=\"font-size: 13px; color: #374151;\">\n"
"    <t t-set=\"recurrent\" t-value=\"object.recurrence_id and not "
"ctx.get('calendar_template_ignore_recurrence')\"/>\n"
"    <t t-set=\"mail_tz\" t-value=\"object._get_mail_tz() or "
"ctx.get('mail_tz')\"/>\n"
"    <t t-set=\"appointment_booker_name\" t-"
"value=\"object.appointment_booker_id.display_name\"/>\n"
"    <h1 style=\"font-size: 18px; font-weight: bold; color: #374151; margin-"
"bottom: 28px;\">\n"
"        Appointment canceled\n"
"        <img src=\"/calendar/static/src/img/times-circle.png\" "
"style=\"border:0; width: 24px; height: 24px; margin-left: 10px; text-"
"decoration:none; vertical-align: bottom;\"/>\n"
"    </h1>\n"
"    <p style=\"margin: 0;\">\n"
"        <t t-if=\"appointment_booker_name\">\n"
"            Hello <t t-out=\"appointment_booker_name\"/>,\n"
"        </t>\n"
"        <t t-else=\"\">\n"
"            Hello,\n"
"        </t>\n"
"        <br/>\n"
"        <span style=\"display: block; margin-top: 8px;\">Your appointment "
"for <t t-out=\"object.appointment_type_id.name or ''\">Technical Demo</t> <t "
"t-if=\"object.appointment_type_id.category not in ['custom', 'anytime'] and "
"object.appointment_type_id.schedule_based_on == 'users' and "
"object.partner_id\"> with <t t-out=\"object.partner_id.name or ''\">Brandon "
"Freeman</t></t> has been canceled.</span>\n"
"    </p>\n"
"    <div style=\"margin: 32px 0 0;\">\n"
"        <h2 t-attf-style=\"margin-bottom: 12px; font-size: 14px; font-"
"weight: bold; color: {{user.company_id.email_secondary_color or '#875A7B'}};"
"\">Details</h2>\n"
"        <table cellpadding=\"0\" cellspacing=\"0\" border=\"0\" style=\"font-"
"size: 13px; color: #374151;\">\n"
"            <tr>\n"
"                <td style=\"width: 100px; color: #676F7B;\">Date &amp; Time</"
"td>\n"
"                <td style=\"font-weight: bold;\">\n"
"                    <t t-out=\"format_datetime(dt=object.start, tz=mail_tz "
"if not object.allday else None, dt_format='EEEE', lang_code=object.env.lang) "
"or ''\">Wednesday</t>\n"
"                    <t t-out=\"format_datetime(dt=object.start, tz=mail_tz "
"if not object.allday else None, dt_format='d', lang_code=object.env.lang) or "
"''\">1</t>\n"
"                    <t t-out=\"format_datetime(dt=object.start, tz=mail_tz "
"if not object.allday else None, dt_format='MMMM y', "
"lang_code=object.env.lang) or ''\">January 2020</t>\n"
"                    <t t-if=\"not object.allday\">\n"
"                        <t t-out=\"format_time(time=object.start, "
"tz=mail_tz, time_format='short', lang_code=object.env.lang) or ''\">8:00</"
"t>\n"
"                        <t t-if=\"not "
"object.appointment_type_id.hide_timezone and mail_tz\">\n"
"                            <span style=\"font-size: 11px; font-weight: "
"normal;\">\n"
"                                (<t t-out=\"mail_tz\"/>)\n"
"                            </span>\n"
"                        </t>\n"
"                    </t>\n"
"                </td>\n"
"            </tr>\n"
"            <tr t-if=\"recurrent\">\n"
"                <td style=\"width: 100px; padding-top: 6px; color: #676F7B;"
"\">When</td>\n"
"                <td style=\"padding-top: 6px;\"><t t-"
"out=\"object.recurrence_id.name or ''\">Every 1 Weeks, for 3 events</t></"
"td>\n"
"            </tr>\n"
"            <tr t-if=\"not object.appointment_type_id.hide_duration and not "
"object.allday and object.duration\" style=\"padding-top: 6px;\">\n"
"                <td style=\"width: 100px; padding-top: 6px; color: #676F7B;"
"\">Duration</td>\n"
"                <td style=\"padding-top: 6px;\"><t t-out=\"('%dH%02d' % "
"(object.duration,round(object.duration*60)%60)) or ''\">0H30</t></td>\n"
"            </tr>\n"
"            <tr t-if=\"object.location\">\n"
"                <td style=\"width: 100px; padding-top: 6px; color: #676F7B; "
"vertical-align: top;\">Location</td>\n"
"                <td style=\"padding-top: 6px;\">\n"
"                    <t t-out=\"object.location or ''\">Bruxelles</t>\n"
"                    <a target=\"_blank\" t-if=\"object.location != "
"object.appointment_type_id.location_id.name\" t-attf-href=\"http://"
"maps.google.com/maps?oi=map&amp;q={{ object.location }}\" style=\"margin-"
"left: 12px; font-size: 12px; color: #008f8c; text-decoration: underline;"
"\">View Map</a>\n"
"                </td>\n"
"            </tr>\n"
"        </table>\n"
"    </div>\n"
"    <div style=\"margin: 32px 0 0;\">\n"
"        <h2 t-attf-style=\"margin-bottom: 12px; font-size: 14px; font-"
"weight: bold; color: {{user.company_id.email_secondary_color or '#875A7B'}};"
"\">Attendees</h2>\n"
"        <div t-foreach=\"object.attendee_ids\" t-as=\"attendee\" "
"style=\"margin-bottom: 5px;\">\n"
"            <img t-if=\"attendee.state\" t-attf-src=\"/calendar/static/src/"
"img/state_{{ attendee.state }}.png\" style=\"border:0; width: 15px; height: "
"15px; vertical-align: text-bottom;\"/>\n"
"            <t t-if=\"attendee.common_name\">\n"
"                <span style=\"margin-left: 5px\" t-"
"out=\"attendee.common_name or ''\">Mitchell Admin</span>\n"
"            </t>\n"
"            <t t-else=\"\">\n"
"                <span style=\"margin-left:5px\">You</span>\n"
"            </t>\n"
"        </div>\n"
"    </div>\n"
"    <div t-if=\"not is_html_empty(object.description)\" style=\"margin: 32px "
"0 0;\">\n"
"        <h2 t-attf-style=\"margin-bottom: 12px; font-size: 14px; font-"
"weight: bold; color: {{user.company_id.email_secondary_color or '#875A7B'}};"
"\">Description of the event</h2>\n"
"        <div t-out=\"object.description\"/>\n"
"    </div>\n"
"</div>\n"
"            "
