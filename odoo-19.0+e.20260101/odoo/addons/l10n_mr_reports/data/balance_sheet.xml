<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="l10n_mr_balance_sheet" model="account.report">
        <field name="name">Balance sheet</field>
        <field name="name@fr">Bilan</field>
        <field name="root_report_id" ref="account_reports.balance_sheet"/>
        <field name="filter_unfold_all" eval="True"/>
        <field name="country_id" ref="base.mr"/>
        <field name="filter_multi_company">selector</field>
        <field name="column_ids">
            <record id="l10n_mr_balance_sheet_columns" model="account.report.column">
                <field name="name">Balance</field>
                <field name="name@fr">Solde</field>
                <field name="expression_label">balance</field>
            </record>
        </field>
        <field name="line_ids">
            <record id="account_financial_report_mr_active_title" model="account.report.line">
                <field name="name">ACTIVE</field>
                <field name="name@fr">ACTIF</field>
                <field name="hierarchy_level">0</field>
                    <field name="expression_ids">
                        <record id="account_financial_report_mr_active_balance" model="account.report.expression">
                            <field name="label">balance</field>
                            <field name="engine">aggregation</field>
                            <field name="formula">MR_fixed_asset.balance + MR_current_asset.balance + MR_adjustment_accounts.balance</field>
                        </record>
                    </field>
                    <field name="children_ids">
                        <record id="account_financial_report_mr_fixed_asset" model="account.report.line">
                            <field name="name">Fixed assets</field>
                            <field name="name@fr">Actif immobilisé</field>
                            <field name="code">MR_fixed_asset</field>
                            <field name="expression_ids">
                                <record id="account_financial_report_mr_fixed_asset_expr" model="account.report.expression">
                                    <field name="label">balance</field>
                                    <field name="engine">aggregation</field>
                                    <field name="formula">MR_intangible_asset.balance + MR_tangible_asset.balance + MR_fixed_in_progress_asset.balance + MR_financial_asset.balance</field>
                                </record>
                            </field>
                            <field name="children_ids">
                                <record id="account_financial_report_mr_intangible_asset" model="account.report.line">
                                    <field name="name">Intangible assets</field>
                                    <field name="name@fr">Immobilisations incorporelles</field>
                                    <field name="code">MR_intangible_asset</field>
                                    <field name="groupby">account_id</field>
                                    <field name="foldable" eval="True"/>
                                    <field name="account_codes_formula">2013 + 2030 + 205 + 2370 + 280 + 2905</field>
                                </record>
                                <record id="account_financial_report_mr_tangible_asset" model="account.report.line">
                                    <field name="name">Tangible assets</field>
                                    <field name="name@fr">Immobilisations corporelles</field>
                                    <field name="code">MR_tangible_asset</field>
                                    <field name="expression_ids">
                                        <record id="account_financial_report_mr_tangible_asset_expr" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">MR_tangible_asset_land.balance + MR_tangible_asset_construction.balance + MR_tangible_asset_op_equipment.balance + MR_tangible_asset_transport_equipment.balance + MR_tangible_asset_office_equipment.balance + MR_tangible_asset_other.balance</field>
                                        </record>
                                    </field>
                                    <field name="children_ids">
                                        <record id="account_financial_report_mr_tangible_asset_land" model="account.report.line">
                                            <field name="name">Land</field>
                                            <field name="name@fr">Terrains</field>
                                            <field name="code">MR_tangible_asset_land</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">210 + 211 + 2120 + 2811 + 291 + 2900</field>
                                        </record>
                                        <record id="account_financial_report_mr_tangible_asset_construction" model="account.report.line">
                                            <field name="name">Construction</field>
                                            <field name="name@fr">Constructions</field>
                                            <field name="code">MR_tangible_asset_construction</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">2122 + 2125 + 213 + 2812</field>
                                        </record>
                                        <record id="account_financial_report_mr_tangible_asset_op_equipment" model="account.report.line">
                                            <field name="name">Operating Equipment</field>
                                            <field name="name@fr">Matériel d'exploitation</field>
                                            <field name="code">MR_tangible_asset_op_equipment</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">214 + 2814</field>
                                        </record>
                                        <record id="account_financial_report_mr_tangible_asset_transport_equipment" model="account.report.line">
                                            <field name="name">Transport Equipment</field>
                                            <field name="name@fr">Matériel de transport</field>
                                            <field name="code">MR_tangible_asset_transport_equipment</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">215 + 2813 + 2815</field>
                                        </record>
                                        <record id="account_financial_report_mr_tangible_asset_office_equipment" model="account.report.line">
                                            <field name="name">Office and computer equipment</field>
                                            <field name="name@fr">Matériel de bureau et informatique</field>
                                            <field name="code">MR_tangible_asset_office_equipment</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">216 + 218 + 2816 + 2817 + 2818</field>
                                        </record>
                                        <record id="account_financial_report_mr_tangible_asset_other" model="account.report.line">
                                            <field name="name">Other tangible fixed assets</field>
                                            <field name="name@fr">Autres immobilisations corporelles</field>
                                            <field name="code">MR_tangible_asset_other</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">200 + 217 + 220 + 268 + 282 + 292</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="account_financial_report_mr_in_progress_asset" model="account.report.line">
                                    <field name="name">Fixed assets in progress</field>
                                    <field name="name@fr">Immobilisations en cours</field>
                                    <field name="code">MR_fixed_in_progress_asset</field>
                                    <field name="groupby">account_id</field>
                                    <field name="foldable" eval="True"/>
                                    <field name="account_codes_formula">2300 + 28103 + 293</field>
                                </record>
                                <record id="account_financial_report_mr_financial_asset" model="account.report.line">
                                    <field name="name">Financial assets</field>
                                    <field name="name@fr">Immobilisations financières</field>
                                    <field name="code">MR_financial_asset</field>
                                    <field name="expression_ids">
                                        <record id="account_financial_report_mr_financial_asset_expr" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">MR_financial_asset_equity.balance + MR_financial_asset_other.balance</field>
                                        </record>
                                    </field>
                                    <field name="children_ids">
                                        <record id="account_financial_report_mr_financial_asset_equity" model="account.report.line">
                                            <field name="name">Equity securities</field>
                                            <field name="name@fr">Titres de participation</field>
                                            <field name="code">MR_financial_asset_equity</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">27 + 297</field>
                                        </record>
                                        <record id="account_financial_report_mr_financial_asset_other" model="account.report.line">
                                            <field name="name">Other financial assets</field>
                                            <field name="name@fr">Autres immobilisations financières</field>
                                            <field name="code">MR_financial_asset_other</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">238 + 260 + 264 + 265 + 296</field>
                                        </record>
                                    </field>
                                </record>
                            </field>
                        </record>
                        <record id="account_financial_report_mr_current_asset" model="account.report.line">
                            <field name="name">Current Assets</field>
                            <field name="name@fr">Actif circulant</field>
                            <field name="code">MR_current_asset</field>
                            <field name="expression_ids">
                                <record id="account_financial_report_mr_current_asset_expr" model="account.report.expression">
                                    <field name="label">balance</field>
                                    <field name="engine">aggregation</field>
                                    <field name="formula">MR_operating_values.balance + MR_short_term_values.balance + MR_available_values.balance</field>
                                </record>
                            </field>
                            <field name="children_ids">
                                <record id="account_financial_report_mr_current_asset_operating" model="account.report.line">
                                    <field name="name">Operating Values</field>
                                    <field name="name@fr">Valeurs d'exploitation</field>
                                    <field name="code">MR_operating_values</field>
                                    <field name="expression_ids">
                                        <record id="account_financial_report_mr_current_asset_operating_expr" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">MR_stocks.balance + MR_operating_in_process.balance</field>
                                        </record>
                                    </field>
                                    <field name="children_ids">
                                        <record id="account_financial_report_mr_stocks" model="account.report.line">
                                            <field name="name">Stocks</field>
                                            <field name="name@fr">Stocks</field>
                                            <field name="code">MR_stocks</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">30 + 31 + 32 + 34 + 35 + 36 + 38 + 390 + 391 + 392 + 394 + 395</field>
                                        </record>
                                            <record id="account_financial_report_mr_operating_in_process" model="account.report.line">
                                            <field name="name">In progress</field>
                                            <field name="name@fr">En-cours</field>
                                            <field name="code">MR_operating_in_process</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">33 + 393</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="account_financial_report_mr_current_asset_short_term" model="account.report.line">
                                    <field name="name">Short-term achievable values</field>
                                    <field name="name@fr">Valeurs réalisables à court terme</field>
                                    <field name="code">MR_short_term_values</field>
                                    <field name="expression_ids">
                                        <record id="account_financial_report_mr_current_asset_short_term_expr" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">MR_debtor_supplier.balance + MR_customers_accounts.balance + MR_staff_accounts.balance + MR_state_orgs.balance + MR_partners_assets.balance + MR_various_debtors.balance</field>
                                        </record>
                                    </field>
                                    <field name="children_ids">
                                        <record id="account_financial_report_mr_debtor_supplier" model="account.report.line">
                                            <field name="name">Debtor suppliers</field>
                                            <field name="name@fr">Fournisseurs débiteurs</field>
                                            <field name="code">MR_debtor_supplier</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">412 + 417 + 418</field>
                                        </record>
                                        <record id="account_financial_report_mr_customers_accounts" model="account.report.line">
                                            <field name="name">Customers and related accounts</field>
                                            <field name="name@fr">Clients et comptes rattachés</field>
                                            <field name="code">MR_customers_accounts</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">410 + 415 +419 + 491</field>
                                        </record>
                                        <record id="account_financial_report_mr_staff_accounts" model="account.report.line">
                                            <field name="name">Staff and related accounts</field>
                                            <field name="name@fr">Personnel et comptes rattachés</field>
                                            <field name="code">MR_staff_accounts</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">425 + 4287</field>
                                        </record>
                                        <record id="account_financial_report_mr_state_orgs" model="account.report.line">
                                            <field name="name">State, communities and social organizations</field>
                                            <field name="name@fr">État, collectivités et organismes sociaux</field>
                                            <field name="code">MR_state_orgs</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">4311 + 4350 + 43567 + 43581 + 43582 + 43586</field>
                                        </record>
                                        <record id="account_financial_report_mr_partners_assets" model="account.report.line">
                                            <field name="name">Partners and group</field>
                                            <field name="name@fr">Associés et groupe</field>
                                            <field name="code">MR_partners_assets</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">450 + 451 + 452 + 453 + 454 + 458 + 495</field>
                                        </record>
                                        <record id="account_financial_report_mr_various_debtors" model="account.report.line">
                                            <field name="name">Various debtors</field>
                                            <field name="name@fr">Débiteurs divers</field>
                                            <field name="code">MR_various_debtors</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">462 + 4687 + 4688 + 490 + 496</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="account_financial_report_mr_current_asset_available_values" model="account.report.line">
                                    <field name="name">Available Values</field>
                                    <field name="name@fr">Valeurs disponibles</field>
                                    <field name="code">MR_available_values</field>
                                    <field name="expression_ids">
                                        <record id="account_financial_report_mr_current_asset_available_values_expr" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">MR_short_term_loans.balance + MR_investment_securities.balance + MR_banks.balance + MR_cash.balance</field>
                                        </record>
                                    </field>
                                    <field name="children_ids">
                                        <record id="account_financial_report_mr_short_term_loans" model="account.report.line">
                                            <field name="name">Short term loans</field>
                                            <field name="name@fr">Prêts à court terme</field>
                                            <field name="code">MR_short_term_loans</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">50 + 51 + 591</field>
                                        </record>
                                        <record id="account_financial_report_mr_investment_securities" model="account.report.line">
                                            <field name="name">Investment securities</field>
                                            <field name="name@fr">Valeurs mobilières de placement</field>
                                            <field name="code">MR_investment_securities</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">52 + 592</field>
                                        </record>
                                        <record id="account_financial_report_mr_banks" model="account.report.line">
                                            <field name="name">Banks</field>
                                            <field name="name@fr">Banques</field>
                                            <field name="code">MR_banks</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">55</field>
                                        </record>
                                        <record id="account_financial_report_mr_cash" model="account.report.line">
                                            <field name="name">Cash</field>
                                            <field name="name@fr">Caisse</field>
                                            <field name="code">MR_cash</field>
                                            <field name="groupby">account_id</field>
                                            <field name="foldable" eval="True"/>
                                            <field name="account_codes_formula">54 + 56 + 57 + 58 + 590</field>
                                        </record>
                                    </field>
                                </record>
                            </field>
                        </record>
                        <record id="account_financial_report_mr_adjustment_accounts" model="account.report.line">
                            <field name="name">Adjustment accounts</field>
                            <field name="name@fr">Comptes de régularisation</field>
                            <field name="code">MR_adjustment_accounts</field>
                            <field name="expression_ids">
                                <record id="account_financial_report_mr_adjustment_accounts_expr" model="account.report.expression">
                                    <field name="label">balance</field>
                                    <field name="engine">aggregation</field>
                                    <field name="formula">MR_prepaid_expenses.balance + MR_conversion_diff.balance + MR_suspense_regularization.balance</field>
                                </record>
                            </field>
                            <field name="children_ids">
                                <record id="account_financial_report_mr_prepaid_expenses" model="account.report.line">
                                    <field name="name">Prepaid Expenses</field>
                                    <field name="name@fr">Charges constatées d'avance</field>
                                    <field name="code">MR_prepaid_expenses</field>
                                    <field name="groupby">account_id</field>
                                    <field name="foldable" eval="True"/>
                                    <field name="account_codes_formula">480</field>
                                </record>
                                <record id="account_financial_report_mr_conversion_diff" model="account.report.line">
                                    <field name="name">Conversion differences - Active</field>
                                    <field name="name@fr">Différences de conversion - Actif</field>
                                    <field name="code">MR_conversion_diff</field>
                                    <field name="groupby">account_id</field>
                                    <field name="foldable" eval="True"/>
                                    <field name="account_codes_formula">484</field>
                                </record>
                                <record id="account_financial_report_mr_suspense_regularization" model="account.report.line">
                                    <field name="name">Suspense and regularization accounts</field>
                                    <field name="name@fr">Comptes d'attente et à régulariser</field>
                                    <field name="code">MR_suspense_regularization</field>
                                    <field name="groupby">account_id</field>
                                    <field name="foldable" eval="True"/>
                                    <field name="account_codes_formula">4866 + 488</field>
                                </record>
                            </field>
                        </record>
                    </field>
            </record>
            <record id="account_financial_report_mr_passive_title" model="account.report.line">
                <field name="name">PASSIVE</field>
                <field name="name@fr">PASSIF</field>
                <field name="hierarchy_level">0</field>
                <field name="expression_ids">
                    <record id="account_financial_report_mr_passive_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">MR_equity.balance + MR_loans_debts.balance + MR_passive_adjust_accs.balance</field>
                    </record>
                </field>
                <field name="children_ids">
                    <record id="account_financial_report_mr_equity" model="account.report.line">
                        <field name="name">Equity</field>
                        <field name="name@fr">Capitaux propres</field>
                        <field name="code">MR_equity</field>
                        <field name="expression_ids">
                            <record id="account_financial_report_mr_equity_expr" model="account.report.expression">
                                <field name="label">balance</field>
                                <field name="engine">aggregation</field>
                                <field name="formula">MR_equity_net.balance + MR_equity_equipment.balance + MR_equity_revaluation_diff.balance + MR_equity_revaluation_prov.balance + MR_equity_regulated_prov.balance</field>
                            </record>
                        </field>
                        <field name="children_ids">
                            <record id="account_financial_report_mr_equity_net" model="account.report.line">
                                <field name="name">Net situation</field>
                                <field name="name@fr">Situation nette</field>
                                <field name="code">MR_equity_net</field>
                                <field name="expression_ids">
                                    <record id="account_financial_report_mr_equity_net_expr" model="account.report.expression">
                                        <field name="label">balance</field>
                                        <field name="engine">aggregation</field>
                                        <field name="formula">MR_equity_capital.balance + MR_equity_reserves.balance + MR_equity_carry_fwd.balance + MR_equity_result_year.balance</field>
                                    </record>
                                </field>
                                <field name="children_ids">
                                    <record id="account_financial_report_mr_equity_capital" model="account.report.line">
                                        <field name="name">Capital</field>
                                        <field name="name@fr">Capital</field>
                                        <field name="code">MR_equity_capital</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">10 + 1500</field>
                                    </record>
                                    <record id="account_financial_report_mr_equity_reserves" model="account.report.line">
                                        <field name="name">Reserves</field>
                                        <field name="name@fr">Réserves</field>
                                        <field name="code">MR_equity_reserves</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">11</field>
                                    </record>
                                    <record id="account_financial_report_mr_equity_carry_fwd" model="account.report.line">
                                        <field name="name">Carry forward</field>
                                        <field name="name@fr">Report à nouveau</field>
                                        <field name="code">MR_equity_carry_fwd</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">12</field>
                                    </record>
                                    <record id="account_financial_report_mr_equity_result_year" model="account.report.line">
                                        <field name="name">Result of the fiscal year</field>
                                        <field name="name@fr">Résultat de l'exercice</field>
                                        <field name="code">MR_equity_result_year</field>
                                        <field name="groupby">account_id</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">13</field>
                                    </record>
                                </field>
                            </record>
                            <record id="account_financial_report_mr_equity_equipment" model="account.report.line">
                                <field name="name">Equipment grants</field>
                                <field name="name@fr">Subventions d'équipement</field>
                                <field name="code">MR_equity_equipment</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="account_codes_formula">14</field>
                            </record>
                            <record id="account_financial_report_mr_equity_revaluation_diff" model="account.report.line">
                                <field name="name">Revaluation differences</field>
                                <field name="name@fr">Ecarts de réévaluation</field>
                                <field name="code">MR_equity_revaluation_diff</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="account_codes_formula">151</field>
                            </record>
                            <record id="account_financial_report_mr_equity_revaluation_prov" model="account.report.line">
                                <field name="name">Special revaluation provision</field>
                                <field name="name@fr">Provision spéciale de réévaluation</field>
                                <field name="code">MR_equity_revaluation_prov</field>
                                <field name="groupby">account_id</field>
                                <field name="foldable" eval="True"/>
                                <field name="account_codes_formula">1508</field>
                            </record>
                            <record id="account_financial_report_mr_equity_regulated_prov" model="account.report.line">
                                <field name="name">Regulated provisions</field>
                                <field name="name@fr">Provisions réglementées</field>
                                <field name="code">MR_equity_regulated_prov</field>
                                <field name="groupby">account_id</field>
