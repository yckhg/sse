<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="dk_tax_return_type" model="account.return.type">
            <field name="name">Tax</field>
            <field name="report_id" ref="l10n_dk.account_tax_report_skat_dk"/>
            <field name="country_id" ref="base.dk" />
            <field name="states_workflow">generic_state_review_submit</field>
        </record>
    </data>

    <record id="dk_ec_sales_list_return_type" model="account.return.type">
        <field name="name">EC Sales List</field>
        <field name="report_id" ref="l10n_dk_reports.denmark_ec_sales_report"/>
        <field name="default_deadline_periodicity">monthly</field>
        <field name="auto_generate" eval="False"/>
        <field name="default_deadline_days_delay">25</field>
    </record>
</odoo>
