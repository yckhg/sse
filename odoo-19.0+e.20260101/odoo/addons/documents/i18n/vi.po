#. module: documents
#: model:mail.template,body_html:documents.mail_template_document_share
msgid ""
"<t t-set=\"documents\" t-value=\"ctx.get('documents', "
"object.env['documents.document'])\"/>\n"
"                <table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" "
"style=\"padding-top: 16px; background-color: #F1F1F1; font-family:Verdana, "
"Arial,sans-serif; color: #454748; width: 100%; border-collapse:separate;"
"\"><tr><td align=\"center\">\n"
"                    <table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" "
"width=\"590\" style=\"padding: 16px; background-color: white; color: "
"#454748; border-collapse:separate;\">\n"
"                    <tbody>\n"
"                        <!-- HEADER -->\n"
"                        <tr>\n"
"                            <td align=\"center\" style=\"min-width: 590px;"
"\">\n"
"                                <table border=\"0\" cellpadding=\"0\" "
"cellspacing=\"0\" width=\"590\" style=\"min-width: 590px; background-color: "
"white; padding: 0px 8px 0px 8px; border-collapse:separate;\">\n"
"                                    <tr><td valign=\"middle\">\n"
"                                        <span style=\"font-size: 10px;\" t-"
"if=\"len(documents) == 1\">\n"
"                                            Shared Document<br/>\n"
"                                            <span style=\"font-size: 20px; "
"font-weight: bold;\" t-out=\"documents.name or ''\"/>\n"
"                                        </span>\n"
"                                        <span style=\"font-size: 10px;\" t-"
"if=\"len(documents) &gt; 1\">\n"
"                                            Shared Documents<br/>\n"
"                                            <span style=\"font-size: 20px; "
"font-weight: bold;\" t-out=\"', "
"'.join(documents.filtered('name').mapped('name'))\"/>\n"
"                                        </span>\n"
"                                        <br/>\n"
"                                    </td><td valign=\"middle\" "
"align=\"right\" t-if=\"not user.company_id.uses_default_logo\">\n"
"                                        <img t-attf-src=\"/logo.png?"
"company={{ user.company_id.id }}\" style=\"padding: 0px; margin: 0px; "
"height: auto; width: 80px;\" t-att-alt=\"user.company_id.name\"/>\n"
"                                    </td></tr>\n"
"                                    <tr><td colspan=\"2\" style=\"text-"
"align:center;\">\n"
"                                      <hr width=\"100%\" style=\"background-"
"color:rgb(204,204,204);border:medium none;clear:both;display:block;font-"
"size:0px;min-height:1px;line-height:0; margin: 16px 0px 16px 0px;\"/>\n"
"                                    </td></tr>\n"
"                                </table>\n"
"                            </td>\n"
"                        </tr>\n"
"                        <!-- CONTENT -->\n"
"                        <tr>\n"
"                            <td align=\"center\" style=\"min-width: 590px;"
"\">\n"
"                                <table border=\"0\" cellpadding=\"0\" "
"cellspacing=\"0\" width=\"590\" style=\"min-width: 590px; background-color: "
"white; padding: 0px 8px 0px 8px; border-collapse:separate;\">\n"
"                                    <tr><td valign=\"top\" style=\"font-"
"size: 13px;\">\n"
"                                        <div>\n"
"                                            Hello <t t-out=\"object.name\"/"
">,\n"
"                                            <br/><br/>\n"
"                                            <t t-if=\"len(documents) &lt;= "
"1\">\n"
"                                                <t t-if=\"documents.type == "
"'folder'\" t-set=\"share_message\">shared the following folder with you</t>\n"
"                                                <t t-else=\"\" t-"
"set=\"share_message\">shared the following document with you</t>\n"
"                                            </t>\n"
"                                            <t t-elif=\"len(documents) &gt; "
"1\">\n"
"                                                <t t-set=\"document_types\" "
"t-value=\"set(documents.mapped('type'))\"/>\n"
"                                                <t t-if=\"document_types == "
"{'folder'}\" t-set=\"share_message\">shared the following folders with you</"
"t>\n"
"                                                <t t-elif=\"'folder' not in "
"document_types\" t-set=\"share_message\">shared the following documents with "
"you</t>\n"
"                                                <t t-else=\"\" t-"
"set=\"share_message\">shared the following documents/folders with you</t>\n"
"                                            </t>\n"
"                                            <t t-out=\"user.name or "
"''\">OdooBot</t> (<t t-out=\"user.email or ''\">odoobot@example.com</t>) <t "
"t-out=\"share_message\"/>:\n"
"                                            <br/><br/>\n"
"                                            <center t-if=\"len(documents) "
"&lt;= 1\">\n"
"                                                <div t-if=\"len(documents) "
"== 1\">\n"
"                                                    <b t-"
"out=\"documents.name\"/>\n"
"                                                </div>\n"
"                                                <div t-"
"if=\"ctx.get('message')\">\n"
"                                                    <i t-out=\"ctx['message']"
"\"/>\n"
"                                                </div>\n"
"                                                <br/>\n"
"                                                <div style=\"margin: 16px "
"0px 16px 0px;\">\n"
"                                                    <a t-att-"
"href=\"documents.access_url\" t-attf-style=\"background-color: "
"{{user.company_id.email_secondary_color or '#875A7B'}}; padding: 20px 30px "
"20px 30px; text-decoration: none; color: "
"{{user.company_id.email_primary_color or '#FFFFFF'}}; border-radius: 5px; "
"font-size:13px;\">\n"
"                                                        <b>Get your files</"
"b>\n"
"                                                    </a>\n"
"                                                </div>\n"
"                                            </center>\n"
"                                            <t t-elif=\"len(documents) &gt; "
"1\">\n"
"                                                <div t-"
"if=\"ctx.get('message')\">\n"
"                                                    <i t-out=\"ctx['message']"
"\"/>\n"
"                                                </div>\n"
"                                                <ul>\n"
"                                                    <li t-"
"foreach=\"documents\" t-as=\"document\">\n"
"                                                        <a t-att-"
"href=\"document.access_url\" t-attf-style=\"color: "
"{{user.company_id.email_secondary_color or '#875A7B'}}; font-size:13px;\">\n"
"                                                            <b>Get <t t-"
"out=\"document.name\"/></b>\n"
"                                                        </a>\n"
"                                                    </li>\n"
"                                                </ul>\n"
"                                            </t>\n"
"\n"
"                                            <br/>\n"
"                                            <t t-if=\"user and "
"user.signature\" data-o-mail-quote-container=\"1\">\n"
"                                                <br/>\n"
"                                                <div data-o-mail-"
"quote=\"1\">--<br data-o-mail-quote=\"1\"/><t t-out=\"user.signature\" data-"
"o-mail-quote=\"1\"/></div>\n"
"                                                <br data-o-mail-quote=\"1\"/"
">\n"
"                                            </t>\n"
"                                        </div>\n"
"                                    </td></tr>\n"
"                                    <tr><td style=\"text-align:center;\">\n"
"                                      <hr width=\"100%\" style=\"background-"
"color:rgb(204,204,204);border:medium none;clear:both;display:block;font-"
"size:0px;min-height:1px;line-height:0; margin: 16px 0px 16px 0px;\"/>\n"
"                                    </td></tr>\n"
"                                </table>\n"
"                            </td>\n"
"                        </tr>\n"
