<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ee_tax_return_type" model="account.return.type">
            <field name="name">KMD Report</field>
            <field name="report_id" ref="l10n_ee.tax_report"/>
            <field name="country_id" ref="base.ee" />
            <field name="default_deadline_days_delay">20</field>
        </record>

        <record id="ee_kmd_inf_tax_return_type" model="account.return.type">
            <field name="name">KMD INF Report (EE)</field>
            <field name="report_id" ref="l10n_ee_reports.kmd_inf_report"/>
            <field name="country_id" ref="base.ee" />
            <field name="default_deadline_days_delay">20</field>
            <field name="states_workflow">generic_state_review_submit</field>
        </record>

        <record id="ee_ec_sales_list_return_type" model="account.return.type">
            <field name="name">EC Sales List</field>
            <field name="report_id" ref="l10n_ee_reports.ee_ec_sales_report"/>
            <field name="country_id" ref="base.ee" />
            <field name="auto_generate" eval="False"/>
            <field name="default_deadline_days_delay">20</field>
        </record>
    </data>
</odoo>
